<?xml version="1.0"?>

<rdf:RDF xmlns="http://www.m0ep.de/sioc-service-auth#"
     xml:base="http://www.m0ep.de/sioc-service-auth"
     xmlns:sioc="http://rdfs.org/sioc/ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:sioc_services="http://rdfs.org/sioc/services#"
     xmlns:waa="http://purl.oclc.org/NET/WebApiAuthentication#">


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#require_authentication">
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#AuthenticationMechanism"/>
        <rdfs:domain rdf:resource="http://rdfs.org/sioc/services#Service"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_authentication">
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#AuthenticationMechanism"/>
        <rdfs:domain rdf:resource="http://rdfs.org/sioc/ns#UserAccount"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_credentials">
        <rdfs:range rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
        <rdfs:domain rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#AuthenticationMechanism"/>
    </owl:ObjectProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <owl:Class rdf:about="http://purl.oclc.org/NET/WebApiAuthentication#APIKey">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>

    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#AccessToken">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#ClientId">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#ClientSecret">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>


    <owl:Class rdf:about="http://purl.oclc.org/NET/WebApiAuthentication#Direct">
        <rdfs:subClassOf rdf:resource="hhttp://purl.oclc.org/NET/WebApiAuthentication#AuthenticationMechanism"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://purl.oclc.org/NET/WebApiAuthentication#OAuth">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#AuthenticationMechanism"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://purl.oclc.org/NET/WebApiAuthentication#Password">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#RefreshToken">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://purl.oclc.org/NET/WebApiAuthentication#Username">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#Credentials"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#WebAPI">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/WebApiAuthentication#AuthenticationMechanism"/>
    </owl:Class>
</rdf:RDF>
