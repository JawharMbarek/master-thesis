sioc:Service
    siocsa:has_authentication

sioc:UserAccount
    siocsa:has_authentication

waa:AuthenticationMechanism
    siocsa:has_credentials
        subPropertyOf waa:hasInputCredentials

waa:OAuth
    subClassOf waa:AuthenticationMechanism

waa:Direct
    subClassOf waa:AuthenticationMechanism

waa:WebAPI
    sameAs waa:WebAPIOperation
    subClassOf waa:AuthenticationMechanism


waa:Credentials
    rdf:hasValue

waa:Username
    subClassOf waa:Credentials

waa:Password
    subClassOf waa:Credentials    

waa:APIKey
    subClassOf waa:Credentials

siocsa:AccessToken
    sameAs waa:OauthToken
    subClassOf waa:Credentials

siocsa:RefreshToken
    subClassOf waa:Credentials

siocsa:ClientId
    sameAs waa:OauthConsumerKey
    subClassOf waa:Credentials

siocsa:ClientSecret
    sameAs waa:OauthConsumerSecret
    subClassOf waa:Credentials

