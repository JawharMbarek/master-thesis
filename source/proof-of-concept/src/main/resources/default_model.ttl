        <https://graph.facebook.com/100000490230885> ;
    siocsa:serviceAuthentication [
        a waa:OAuth ;
        siocsa:credentials [
            a siocsa:ClientId ;
            rdf:value "410334369012894"
        ] , [
            a siocsa:ClientSecret ;
            rdf:value "5988954e39fc9ca4d39bd7b374a7d72b"
        ]
    ] .

