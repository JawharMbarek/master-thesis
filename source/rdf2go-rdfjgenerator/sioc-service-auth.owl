<?xml version="1.0"?>

<rdf:RDF xmlns="http://www.m0ep.de/sioc-service-auth#"
     xml:base="http://www.m0ep.de/sioc-service-auth"
     xmlns:sioc="http://rdfs.org/sioc/ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:sioc_services="http://rdfs.org/sioc/services#">


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#require_authentication">
        <rdfs:range rdf:resource="http://www.m0ep.de/sioc-service-auth#Authentication"/>
        <rdfs:domain rdf:resource="http://rdfs.org/sioc/services#Service"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_authentication">
        <rdfs:range rdf:resource="http://www.m0ep.de/sioc-service-auth#Authentication"/>
        <rdfs:domain rdf:resource="http://rdfs.org/sioc/ns#UserAccount"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_credential">
        <rdfs:range rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
        <rdfs:domain rdf:resource="http://www.m0ep.de/sioc-service-auth#Authentication"/>
    </owl:ObjectProperty>
    


    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#oauth_version">
        <rdfs:domain rdf:resource="http://www.m0ep.de/sioc-service-auth#OAuth"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_accesstokenendpoint">
        <rdfs:domain rdf:resource="http://www.m0ep.de/sioc-service-auth#OAuth"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_requesttokenendpoint">
        <rdfs:domain rdf:resource="http://www.m0ep.de/sioc-service-auth#OAuth"/>
    </owl:ObjectProperty>



    <owl:ObjectProperty rdf:about="http://www.m0ep.de/sioc-service-auth#has_authorizationurl">
        <rdfs:domain rdf:resource="http://www.m0ep.de/sioc-service-auth#OAuth"/>
    </owl:ObjectProperty>



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->



    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#APIKey">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#AccessToken">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#Authentication"/>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#ClientId">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#ClientSecret">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#Credential"/>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#Direct">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Authentication"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#OAuth">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Authentication"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#Password">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#RefreshToken">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#Username">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Credential"/>
    </owl:Class>
    


    <owl:Class rdf:about="http://www.m0ep.de/sioc-service-auth#WebAPI">
        <rdfs:subClassOf rdf:resource="http://www.m0ep.de/sioc-service-auth#Authentication"/>
    </owl:Class>
</rdf:RDF>
