				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<!-- Dependencies -->


	<dependencies>
		<dependency>
			<groupId>org.semweb4j</groupId>
