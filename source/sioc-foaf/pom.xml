	<modelVersion>4.0.0</modelVersion>

	<!-- Maven Infos -->

	<groupId>de.m0ep.rdf2go</groupId>
	<artifactId>sioc-foaf</artifactId>
	<version>0.4</version>

	<!-- Project Infos -->

	<name>sioc</name>
	<description>An API for the SIOC and FOAF Format based on RDF2Go and RDFReactor</description>

	<licenses>
		<license>
			<name>The MIT License</name>
			<url>http://fm.mit-license.org/</url>
